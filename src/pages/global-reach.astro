---
import BaseLayout from '../layouts/BaseLayout.astro';
import { Globe, MapPin, Users, TrendingUp, Heart, Smartphone, Target, CheckCircle } from 'lucide-astro';

// Scope stats
const scopeStats = [
  { number: "50+", label: "Countries Served" },
  { number: "4", label: "Continents" },
  { number: "1,000+", label: "Target Schools by 2030" },
  { number: "10M+", label: "Students Impacted (Goal)" }
];

// Africa countries
const africaCountries = [
  { flag: "ZW", name: "Zimbabwe", subtitle: "Home Base", featured: true, points: ["Company headquarters (Harare)", "Primary reinvestment focus", "Local team development", "Pilot school partnerships"] },
  { flag: "KE", name: "Kenya", subtitle: "East Africa Tech Hub", points: ["Large education market (18M students)", "Strong digital infrastructure", "Active EdTech ecosystem"] },
  { flag: "NG", name: "Nigeria", subtitle: "West Africa's Largest Market", points: ["44M students (largest in Africa)", "Growing digital adoption", "Diverse language needs"] },
  { flag: "GH", name: "Ghana", subtitle: "Progressive Education Policy", points: ["Government digital education push", "English-speaking market", "Tech-forward approach"] },
  { flag: "ZA", name: "South Africa", subtitle: "Advanced Infrastructure", points: ["Mature EdTech market", "Broadband connectivity", "Innovation partnerships"] },
  { flag: "TZ", name: "Tanzania", subtitle: "East African Growth Market", points: ["Swahili language support", "Mobile-first population", "Rural school challenges"] },
  { flag: "UG", name: "Uganda", subtitle: "Innovative Education Models", points: ["Community school focus", "Low-cost implementations", "High mobile penetration"] },
  { flag: "ET", name: "Ethiopia", subtitle: "Emerging Market", points: ["Large student population (26M)", "Government reform focus", "Amharic language support"] }
];

// South Asia countries
const southAsiaCountries = [
  { flag: "IN", name: "India", subtitle: "World's Largest Education System", points: ["260M students (K-12)", "Mobile-first (1.2B smartphone users)", "Hindi, English, regional languages", "Budget-conscious schools"] },
  { flag: "BD", name: "Bangladesh", subtitle: "High Mobile Penetration", points: ["18M students", "175M mobile subscribers", "Bengali language support", "Rural connectivity challenges"] },
  { flag: "PK", name: "Pakistan", subtitle: "Growing Digital Infrastructure", points: ["50M students", "Urdu language priority", "Low-cost device solutions"] },
  { flag: "NP", name: "Nepal", subtitle: "Community-Focused Education", points: ["8M students", "Rural school challenges", "Limited connectivity"] }
];

// Southeast Asia countries
const southeastAsiaCountries = [
  { flag: "ID", name: "Indonesia", subtitle: "Archipelago Challenges", points: ["45M students", "17,000+ islands connectivity", "Bahasa Indonesia language"] },
  { flag: "PH", name: "Philippines", subtitle: "High Digital Engagement", points: ["28M students", "English proficiency advantage", "Strong social media adoption"] },
  { flag: "VN", name: "Vietnam", subtitle: "Rapid EdTech Growth", points: ["15M students", "Government digital push", "Tech-savvy population"] },
  { flag: "MM", name: "Myanmar", subtitle: "Emerging Market", points: ["9M students", "Growing mobile infrastructure", "Burmese language support"] }
];

// Latin America countries
const latinAmericaCountries = [
  { flag: "BR", name: "Brazil", subtitle: "Largest Latin American Market", points: ["47M students", "Portuguese language support", "Urban-rural technology gap"] },
  { flag: "MX", name: "Mexico", subtitle: "Spanish Language Hub", points: ["25M students", "High mobile penetration", "Government education investment"] },
  { flag: "CO", name: "Colombia", subtitle: "Innovation Focus", points: ["9M students", "EdTech startup ecosystem", "Digital transformation push"] },
  { flag: "PE", name: "Peru", subtitle: "Remote Learning Needs", points: ["8M students", "Andes connectivity challenges", "Indigenous language support"] }
];

// Pacific & Caribbean countries
const pacificCountries = [
  { flag: "PG", name: "Papua New Guinea", subtitle: "Linguistic Diversity", points: ["800+ languages", "Remote school challenges", "Low-bandwidth solutions"] },
  { flag: "FJ", name: "Fiji", subtitle: "Pacific Hub", points: ["English + Fijian + Hindi", "Island connectivity", "Regional influence"] },
  { flag: "JM", name: "Jamaica", subtitle: "Caribbean Education Leader", points: ["English-speaking market", "Strong education tradition", "Hurricane resilience needs"] },
  { flag: "HT", name: "Haiti", subtitle: "French-Speaking Caribbean", points: ["French & Creole languages", "Limited infrastructure", "High-impact opportunity"] }
];

// Why these regions
const reasons = [
  { icon: Smartphone, title: "Shared Mobile-First Reality", description: "67.5% internet penetration via smartphones in Africa. Similar patterns in Asia (72%), Latin America (69%), and Pacific (64%). Our frameworks are designed for this reality." },
  { icon: Target, title: "Leapfrog Opportunity", description: "Unburdened by legacy infrastructure, developing countries can build digital-first education systems optimized for 2050—not retrofitting 1950s models." },
  { icon: Users, title: "Similar Challenges", description: "Budget constraints, limited 1:1 device access, multilingual needs, urban-rural divides, connectivity issues—shared challenges require shared solutions." },
  { icon: TrendingUp, title: "High-Growth Markets", description: "Young populations, expanding middle class, government education investment, digital infrastructure growth—conditions ripe for transformation." },
  { icon: Heart, title: "Ubuntu Philosophy Alignment", description: "\"I am because we are\" resonates across cultures—community-centered values in Africa, Asia, Latin America, and Pacific align with collaborative education." },
  { icon: CheckCircle, title: "Proven Adaptability", description: "Open frameworks designed to adapt to local contexts: languages, cultural values, curriculum standards, budget realities, infrastructure constraints." }
];

// Impact goals
const impactGoals = [
  { region: "Africa", goals: ["500 schools implementing frameworks", "5M students impacted", "15+ countries with active implementations", "Zimbabwe: 50+ schools (home base focus)"] },
  { region: "South & Southeast Asia", goals: ["300 schools implementing frameworks", "3M students impacted", "8+ countries with active implementations", "Hindi, Bengali, Bahasa support"] },
  { region: "Latin America", goals: ["150 schools implementing frameworks", "1.5M students impacted", "6+ countries with active implementations", "Spanish & Portuguese language support"] },
  { region: "Pacific & Caribbean", goals: ["50 schools implementing frameworks", "500K students impacted", "10+ island nations served", "Offline-capable solutions priority"] }
];
---

<BaseLayout
  title="Global Reach - Empowering Schools Across Developing Countries"
  description="nyuchi learning serves schools across Africa, Asia, Latin America, and the Pacific. Pan-African priority with global developing world focus. Shared challenges, mobile-first reality, leapfrog opportunity."
  keywords="pan-African education, global education framework, developing countries education, Africa schools, Asia education, Latin America schools, Pacific education, mobile-first education global"
>
  <div class="max-w-[1200px] mx-auto px-4 sm:px-6 md:px-8">
    <!-- Hero Section -->
    <section class="bg-gradient-to-br from-[var(--primary)] to-[var(--primary)]/80 text-[var(--text)] py-12 sm:py-16 md:py-24 px-4 sm:px-6 md:px-8 text-center rounded-b-2xl md:rounded-b-3xl mb-8 sm:mb-12 md:mb-16">
      <div class="max-w-[1200px] mx-auto">
        <p class="text-base sm:text-lg opacity-90 mb-3 sm:mb-4 italic">Ubuntu: I am because we are</p>
        <h1 class="text-[clamp(1.75rem,6vw,3.5rem)] font-serif mb-4 sm:mb-6">Global Reach</h1>
        <p class="text-[clamp(1rem,2.5vw,1.35rem)] leading-relaxed max-w-[800px] mx-auto">Empowering schools across Africa, Asia, Latin America, and the Pacific to leapfrog traditional education and build for 2050</p>
      </div>
    </section>

    <!-- Geographic Scope Overview -->
    <section class="py-10 sm:py-12 md:py-16 bg-[var(--bg)]">
      <div class="text-center mb-8 sm:mb-10 md:mb-12">
        <h2 class="text-2xl sm:text-3xl md:text-4xl text-[var(--text)] mb-4 sm:mb-6">Where We Serve</h2>
        <p class="text-base sm:text-lg md:text-xl leading-relaxed text-[var(--text-secondary)] max-w-[900px] mx-auto">
          <strong>nyuchi learning is pan-African with a global developing world focus.</strong>
          While Africa is our home and primary priority, our frameworks are designed for all developing
          countries facing similar challenges: mobile-first populations, limited 1:1 device access,
          budget constraints, and the opportunity to leapfrog legacy educational infrastructure.
        </p>
      </div>

      <div class="grid grid-cols-1 sm:grid-cols-2 lg:grid-cols-4 gap-4 sm:gap-6 md:gap-8 mt-8 sm:mt-10 md:mt-12">
        {scopeStats.map((stat) => (
          <div class="bg-[var(--bg-surface)] p-5 sm:p-6 md:p-8 rounded-xl text-center shadow-sm">
            <div class="text-3xl sm:text-4xl md:text-5xl font-bold text-[var(--primary)] mb-2 font-serif">{stat.number}</div>
            <div class="text-sm sm:text-base text-[var(--text-secondary)]">{stat.label}</div>
          </div>
        ))}
      </div>
    </section>

    <!-- Regional Focus Areas -->
    <section class="py-10 sm:py-12 md:py-16">
      <h2 class="text-2xl sm:text-3xl md:text-4xl text-[var(--text)] text-center mb-8 sm:mb-12 md:mb-16">Regional Focus Areas</h2>

      <!-- Africa (Primary) -->
      <div class="mb-8 sm:mb-12 md:mb-16 bg-[var(--bg-surface)] p-5 sm:p-6 md:p-8 lg:p-12 rounded-xl shadow-sm border-2 border-[var(--primary)]">
        <div class="flex items-start gap-4 sm:gap-6 mb-6 sm:mb-8 pb-6 sm:pb-8 border-b-2 border-[var(--border)]">
          <MapPin class="text-[var(--primary)] flex-shrink-0 w-8 h-8 sm:w-10 sm:h-10" strokeWidth={1.5} />
          <div>
            <h3 class="text-xl sm:text-2xl md:text-3xl text-[var(--text)] mb-2">Africa (Primary Focus)</h3>
            <p class="text-sm sm:text-base text-[var(--text-secondary)]">Home base • Reinvestment commitment • Local ownership</p>
          </div>
        </div>

        <p class="text-base sm:text-lg leading-relaxed mb-6 sm:mb-8 md:mb-10 text-[var(--text-secondary)]"><strong>Africa is our home.</strong> nyuchi africa is Zimbabwe-registered (2019) with a commitment to reinvesting profits into local economies and building solutions tested on African roads, not just theory.</p>

        <div class="grid grid-cols-1 sm:grid-cols-2 lg:grid-cols-4 gap-4 sm:gap-5 md:gap-6 mb-6 sm:mb-8">
          {africaCountries.map((country) => (
            <div class={`bg-[var(--bg)] p-4 sm:p-5 md:p-6 rounded-xl border transition-all duration-300 hover:-translate-y-0.5 hover:shadow-md hover:border-[var(--primary)] ${country.featured ? 'border-2 border-[var(--primary)] bg-[var(--bg-surface)]' : 'border-[var(--border)]'}`}>
              <h4 class="text-lg sm:text-xl text-[var(--text)] mb-2">{country.name}</h4>
              <p class="text-xs sm:text-sm text-[var(--text-secondary)] font-semibold mb-2 sm:mb-3">{country.featured ? <strong>{country.subtitle}</strong> : country.subtitle}</p>
              <ul class="list-none p-0 m-0 space-y-1">
                {country.points.map((point) => (
                  <li class="text-xs sm:text-sm text-[var(--text-secondary)] pl-4 relative before:content-['→'] before:absolute before:left-0 before:text-[var(--primary)]">{point}</li>
                ))}
              </ul>
            </div>
          ))}
        </div>

        <p class="text-xs sm:text-sm text-[var(--text-secondary)] italic"><strong>Also serving:</strong> Rwanda, Senegal, Côte d'Ivoire, Zambia, Malawi, Botswana, Namibia, and other African countries on request.</p>
      </div>

      <!-- South Asia -->
      <div class="mb-8 sm:mb-12 md:mb-16 bg-[var(--bg-surface)] p-5 sm:p-6 md:p-8 lg:p-12 rounded-xl shadow-sm">
        <div class="flex items-start gap-4 sm:gap-6 mb-6 sm:mb-8 pb-6 sm:pb-8 border-b-2 border-[var(--border)]">
          <Globe class="text-[var(--primary)] flex-shrink-0 w-8 h-8 sm:w-10 sm:h-10" strokeWidth={1.5} />
          <div>
            <h3 class="text-xl sm:text-2xl md:text-3xl text-[var(--text)] mb-2">South Asia</h3>
            <p class="text-sm sm:text-base text-[var(--text-secondary)]">Massive scale • Mobile-first populations • Multilingual complexity</p>
          </div>
        </div>

        <div class="grid grid-cols-1 sm:grid-cols-2 lg:grid-cols-4 gap-4 sm:gap-5 md:gap-6">
          {southAsiaCountries.map((country) => (
            <div class="bg-[var(--bg)] p-4 sm:p-5 md:p-6 rounded-xl border border-[var(--border)] transition-all duration-300 hover:-translate-y-0.5 hover:shadow-md hover:border-[var(--primary)]">
              <h4 class="text-lg sm:text-xl text-[var(--text)] mb-2">{country.name}</h4>
              <p class="text-xs sm:text-sm text-[var(--text-secondary)] font-semibold mb-2 sm:mb-3">{country.subtitle}</p>
              <ul class="list-none p-0 m-0 space-y-1">
                {country.points.map((point) => (
                  <li class="text-xs sm:text-sm text-[var(--text-secondary)] pl-4 relative before:content-['→'] before:absolute before:left-0 before:text-[var(--primary)]">{point}</li>
                ))}
              </ul>
            </div>
          ))}
        </div>
      </div>

      <!-- Southeast Asia -->
      <div class="mb-8 sm:mb-12 md:mb-16 bg-[var(--bg-surface)] p-5 sm:p-6 md:p-8 lg:p-12 rounded-xl shadow-sm">
        <div class="flex items-start gap-4 sm:gap-6 mb-6 sm:mb-8 pb-6 sm:pb-8 border-b-2 border-[var(--border)]">
          <Globe class="text-[var(--primary)] flex-shrink-0 w-8 h-8 sm:w-10 sm:h-10" strokeWidth={1.5} />
          <div>
            <h3 class="text-xl sm:text-2xl md:text-3xl text-[var(--text)] mb-2">Southeast Asia</h3>
            <p class="text-sm sm:text-base text-[var(--text-secondary)]">Tech-forward • Diverse economies • Strong mobile adoption</p>
          </div>
        </div>

        <div class="grid grid-cols-1 sm:grid-cols-2 lg:grid-cols-4 gap-4 sm:gap-5 md:gap-6">
          {southeastAsiaCountries.map((country) => (
            <div class="bg-[var(--bg)] p-4 sm:p-5 md:p-6 rounded-xl border border-[var(--border)] transition-all duration-300 hover:-translate-y-0.5 hover:shadow-md hover:border-[var(--primary)]">
              <h4 class="text-lg sm:text-xl text-[var(--text)] mb-2">{country.name}</h4>
              <p class="text-xs sm:text-sm text-[var(--text-secondary)] font-semibold mb-2 sm:mb-3">{country.subtitle}</p>
              <ul class="list-none p-0 m-0 space-y-1">
                {country.points.map((point) => (
                  <li class="text-xs sm:text-sm text-[var(--text-secondary)] pl-4 relative before:content-['→'] before:absolute before:left-0 before:text-[var(--primary)]">{point}</li>
                ))}
              </ul>
            </div>
          ))}
        </div>
      </div>

      <!-- Latin America -->
      <div class="mb-8 sm:mb-12 md:mb-16 bg-[var(--bg-surface)] p-5 sm:p-6 md:p-8 lg:p-12 rounded-xl shadow-sm">
        <div class="flex items-start gap-4 sm:gap-6 mb-6 sm:mb-8 pb-6 sm:pb-8 border-b-2 border-[var(--border)]">
          <Globe class="text-[var(--primary)] flex-shrink-0 w-8 h-8 sm:w-10 sm:h-10" strokeWidth={1.5} />
          <div>
            <h3 class="text-xl sm:text-2xl md:text-3xl text-[var(--text)] mb-2">Latin America</h3>
            <p class="text-sm sm:text-base text-[var(--text-secondary)]">Spanish-speaking markets • Urban-rural divide • Growing digital access</p>
          </div>
        </div>

        <div class="grid grid-cols-1 sm:grid-cols-2 lg:grid-cols-4 gap-4 sm:gap-5 md:gap-6 mb-6 sm:mb-8">
          {latinAmericaCountries.map((country) => (
            <div class="bg-[var(--bg)] p-4 sm:p-5 md:p-6 rounded-xl border border-[var(--border)] transition-all duration-300 hover:-translate-y-0.5 hover:shadow-md hover:border-[var(--primary)]">
              <h4 class="text-lg sm:text-xl text-[var(--text)] mb-2">{country.name}</h4>
              <p class="text-xs sm:text-sm text-[var(--text-secondary)] font-semibold mb-2 sm:mb-3">{country.subtitle}</p>
              <ul class="list-none p-0 m-0 space-y-1">
                {country.points.map((point) => (
                  <li class="text-xs sm:text-sm text-[var(--text-secondary)] pl-4 relative before:content-['→'] before:absolute before:left-0 before:text-[var(--primary)]">{point}</li>
                ))}
              </ul>
            </div>
          ))}
        </div>

        <p class="text-xs sm:text-sm text-[var(--text-secondary)] italic"><strong>Also serving:</strong> Argentina, Chile, Ecuador, Bolivia, Central American countries</p>
      </div>

      <!-- Pacific & Caribbean -->
      <div class="mb-8 sm:mb-12 md:mb-16 bg-[var(--bg-surface)] p-5 sm:p-6 md:p-8 lg:p-12 rounded-xl shadow-sm">
        <div class="flex items-start gap-4 sm:gap-6 mb-6 sm:mb-8 pb-6 sm:pb-8 border-b-2 border-[var(--border)]">
          <Globe class="text-[var(--primary)] flex-shrink-0 w-8 h-8 sm:w-10 sm:h-10" strokeWidth={1.5} />
          <div>
            <h3 class="text-xl sm:text-2xl md:text-3xl text-[var(--text)] mb-2">Pacific & Caribbean</h3>
            <p class="text-sm sm:text-base text-[var(--text-secondary)]">Island nations • Small scale, high impact • Connectivity challenges</p>
          </div>
        </div>

        <div class="grid grid-cols-1 sm:grid-cols-2 lg:grid-cols-4 gap-4 sm:gap-5 md:gap-6 mb-6 sm:mb-8">
          {pacificCountries.map((country) => (
            <div class="bg-[var(--bg)] p-4 sm:p-5 md:p-6 rounded-xl border border-[var(--border)] transition-all duration-300 hover:-translate-y-0.5 hover:shadow-md hover:border-[var(--primary)]">
              <h4 class="text-lg sm:text-xl text-[var(--text)] mb-2">{country.name}</h4>
              <p class="text-xs sm:text-sm text-[var(--text-secondary)] font-semibold mb-2 sm:mb-3">{country.subtitle}</p>
              <ul class="list-none p-0 m-0 space-y-1">
                {country.points.map((point) => (
                  <li class="text-xs sm:text-sm text-[var(--text-secondary)] pl-4 relative before:content-['→'] before:absolute before:left-0 before:text-[var(--primary)]">{point}</li>
                ))}
              </ul>
            </div>
          ))}
        </div>

        <p class="text-xs sm:text-sm text-[var(--text-secondary)] italic"><strong>Also serving:</strong> Samoa, Tonga, Vanuatu, Solomon Islands, other Caribbean nations</p>
      </div>
    </section>

    <!-- Why These Regions -->
    <section class="py-10 sm:py-12 md:py-16 bg-[var(--bg)]">
      <h2 class="text-2xl sm:text-3xl md:text-4xl text-[var(--text)] text-center mb-8 sm:mb-10 md:mb-12">Why These Regions?</h2>
      <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-4 sm:gap-6 md:gap-8">
        {reasons.map((reason) => (
          <div class="bg-[var(--bg-surface)] p-5 sm:p-6 md:p-8 rounded-xl shadow-sm text-center">
            <reason.icon class="text-[var(--primary)] mx-auto mb-3 sm:mb-4 w-8 h-8 sm:w-10 sm:h-10" strokeWidth={1.5} />
            <h3 class="text-lg sm:text-xl text-[var(--text)] mb-3 sm:mb-4">{reason.title}</h3>
            <p class="text-sm sm:text-base text-[var(--text-secondary)] leading-relaxed">{reason.description}</p>
          </div>
        ))}
      </div>
    </section>

    <!-- Impact Goals by Region -->
    <section class="py-10 sm:py-12 md:py-16">
      <h2 class="text-2xl sm:text-3xl md:text-4xl text-[var(--text)] text-center mb-8 sm:mb-10 md:mb-12">2025-2030 Impact Goals by Region</h2>
      <div class="grid grid-cols-1 sm:grid-cols-2 lg:grid-cols-4 gap-4 sm:gap-6 md:gap-8 mb-6 sm:mb-8">
        {impactGoals.map((goal) => (
          <div class="bg-[var(--bg-surface)] p-5 sm:p-6 md:p-8 rounded-xl border-2 border-[var(--primary)] shadow-sm">
            <h3 class="text-xl sm:text-2xl text-[var(--primary)] mb-4 sm:mb-6">{goal.region}</h3>
            <ul class="list-none p-0 m-0 space-y-2 sm:space-y-3">
              {goal.goals.map((item) => (
                <li class="flex items-start gap-2 sm:gap-3 text-sm sm:text-base text-[var(--text-secondary)] leading-relaxed">
                  <CheckCircle class="text-[var(--mineral-malachite)] flex-shrink-0 mt-0.5 w-4 h-4 sm:w-5 sm:h-5" />
                  <span>{item}</span>
                </li>
              ))}
            </ul>
          </div>
        ))}
      </div>

      <p class="text-center text-base sm:text-lg md:text-xl text-[var(--text)] mt-8 sm:mt-10 md:mt-12 p-4 sm:p-5 md:p-6 bg-[var(--bg-surface)] rounded-xl"><strong>Total 2030 Goal:</strong> 1,000 schools, 10 million students across 40+ developing countries</p>
    </section>

    <!-- CTA Section -->
    <section class="text-center py-10 sm:py-14 md:py-20 px-4 sm:px-6 md:px-8 bg-gradient-to-br from-[var(--bg-surface)] to-[var(--bg)] rounded-2xl md:rounded-3xl mb-8 sm:mb-12 md:mb-16">
      <h2 class="text-2xl sm:text-3xl md:text-4xl text-[var(--text)] mb-4 sm:mb-6">Join the Global Movement</h2>
      <p class="text-base sm:text-lg md:text-xl text-[var(--text-secondary)] max-w-[800px] mx-auto mb-6 sm:mb-8 md:mb-10 leading-relaxed">Whether you're in Harare, Mumbai, Jakarta, São Paulo, or Port Moresby—our frameworks are designed for your school. Download, adapt, implement, and share your learnings with the global community.</p>
      <div class="flex flex-col sm:flex-row gap-3 sm:gap-4 md:gap-6 justify-center items-center">
        <a href="/frameworks" class="inline-flex items-center justify-center gap-2 h-12 sm:h-14 px-6 sm:px-8 md:px-10 rounded-xl font-semibold transition-all duration-200 bg-[var(--primary)] text-white hover:-translate-y-0.5 hover:shadow-lg hover:brightness-110 text-base sm:text-lg w-full sm:w-auto">Download Frameworks</a>
        <a href="/resources" class="inline-flex items-center justify-center gap-2 h-12 sm:h-14 px-6 sm:px-8 md:px-10 rounded-xl font-semibold transition-all duration-200 bg-[var(--bg-surface)] text-[var(--primary)] border-2 border-[var(--primary)] hover:bg-[var(--bg)] text-base sm:text-lg w-full sm:w-auto">Browse Resources</a>
        <a href="/consultation" class="inline-flex items-center justify-center gap-2 h-12 sm:h-14 px-6 sm:px-8 md:px-10 rounded-xl font-semibold transition-all duration-200 bg-[var(--bg-surface)] text-[var(--primary)] border-2 border-[var(--primary)] hover:bg-[var(--bg)] text-base sm:text-lg w-full sm:w-auto">Schedule Consultation</a>
      </div>
    </section>
  </div>
</BaseLayout>
